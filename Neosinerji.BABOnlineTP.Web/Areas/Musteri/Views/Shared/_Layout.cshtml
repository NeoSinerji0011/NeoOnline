    <script src="~/Content/js/jquery.validate.js"></script>
    <script src="~/Content/js/jquery.validate.bootstrap.js"></script>
    <script src="~/Content/js/jquery.validate.unobtrusive.js"></script>
    <script src="~/Content/js/jquery.validate.custom.js"></script>
    <script src="~/Content/js/neosinerji.babonlinetp.common.js"></script>
    <script src="~/Content/js/jquery.gritter.min.js"></script>
    <script src="~/Content/js/skype-uri.js" type="text/javascript"></script>
    <script src="~/Content/js/jquery.alphanumeric.pack.js"></script>

    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="~/Content/js/app.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL SCRIPTS -->
    <!--Google Maps -->

    <script src="https://maps.google.com/maps/api/js?v=3.5&sensor=true" type="text/javascript"></script>

    <script src="~/Content/js/Maps/gmaps.js"></script>

