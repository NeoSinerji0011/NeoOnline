@using Neosinerji.BABOnlineTP.Web.Content.Lang
@using Neosinerji.BABOnlineTP.Web.Areas.Teklif.Models
@using Neosinerji.BABOnlineTP.Business
@model List<PotansiyelMusteriFinderOzetModel>

<table id="user-table" class="table table-bordered table-striped responsive data-table">
    <thead>
        <tr>
            <th style="width: 100px;"></th>
            <th style="width: 200px;">@babonline.Customer_Code</th>
            <th style="width: 300px;">@babonline.NameLastname</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="text-align: center;"><a class="btn btn-mini btn-success user-sec-btn" musteri-kodu="@item.MusteriKodu" musteri-adi="@(item.Adi + " " + item.Soyadi)">@babonline.Choose</a></td>
                <td>@item.MusteriKodu</td>
                <td>@(item.Adi + " " + item.Soyadi)</td>
            </tr>
        }
    </tbody>
</table>
