@using Neosinerji.BABOnlineTP.Web.Content.Lang
@using Neosinerji.BABOnlineTP.Web.Areas.PotansiyelMusteri.Models
@using Neosinerji.BABOnlineTP.Web.Tools.Helpers
@model PotansiyelMusteriModel


@section HorizontalMenu
{
    <ul class="nav">

        <li>
            <a href="~/Musteri/Musteri/Liste">@babonline.Customer_Search<span></span></a>
        </li>
        <li class="active">
            <a href="#">@babonline.Customer_Add_List<span class="selected"></span></a>
        </li>
        <li>
            <span class="hor-menu-search-form-toggler">&nbsp;</span>
            <div class="search-form hidden-phone hidden-tablet">
                <form class="form-search">
                    <div class="input-append">

                        <input type="text" placeholder="@babonline.Search" class="m-wrap">
                        <button type="button" class="btn"></button>
                    </div>
                </form>
            </div>
        </li>
    </ul>
}
    <title>PotansiyelMüşteriAktar</title>

@section PageHeader
{

}
<div class="row-fluid">
    <div class="span12">
        <div class="portlet box light-grey tabbable" style="margin-top: 24px;">
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-reorder"></i>
                    <span class="hidden-480">@babonline.Customer_Add_List</span>
                </div>
            </div>
            <div class="portlet-body form">
                @using (Html.BeginForm("Excelaktar", "Musteri", FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
                {
                    Html.EnableClientValidation(true);
                    @Html.ValidationSummary(true, @babonline.Message_GenericErrorHeader)      
  
                    <div class="control-group">
                        <label class="control-label"></label>
                        <div class="controls">
                            <span>@babonline.ExcelTemplate <a href="window.open('https://neobabstoragetest.blob.core.windows.net/musteri-dokuman/178/Musteri_Sablonu%20G%C3%BCncel%20V%20.xlsx');return false;">@babonline.FromHere </a>@babonline.Download</span>
                        </div>
                    </div>
                    <div class="control-group">
                        <label class="control-label">@babonline.File</label>
                        <div class="controls">
                            <input type="file" id="file" name="file" style="width: 300px;" />
                        </div>
                    </div>
                    <div class="form-actions" style="margin-left: 0px !important; margin-right: 0px !important; margin-bottom: 0px !important;">
                        <button tabindex="19" id="btn-kaydet" class="btn btn-success"><i class="icon-ok">&nbsp;</i>@babonline.Save</button>
                        <a class="btn" href="~/Musteri/Musteri/Liste">@babonline.Cancel</a>
                    </div>
                }
            </div>
        </div>
    </div>
</div>




