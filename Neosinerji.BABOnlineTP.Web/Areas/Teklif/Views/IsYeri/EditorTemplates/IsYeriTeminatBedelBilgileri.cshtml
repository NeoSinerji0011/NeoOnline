@using Neosinerji.BABOnlineTP.Web.Content.Lang;
@model Neosinerji.BABOnlineTP.Web.Areas.Teklif.Models.IsYeriTeminatBedelBilgileri



<div class="row-fluid">
    <div class="span6">
        <div class="control-group">
            <label class="control-label">@babonline.AssetValue<span class="required">*</span></label>
            <div class="controls">
                <div class="input-append">
                    @Html.TextBoxFor(m => m.DemirbasBedeli, new { @class = "m-wrap autoNumeric", style = "width:90px; text-align:right" })<span class="add-on"> TL</span>
                </div>
                @Html.ValidationMessageFor(s => s.DemirbasBedeli)
            </div>
        </div>
    </div>
    <div class="span6">
        <div class="control-group">
            <label class="control-label">@babonline.BuildingCost</label>
            <div class="controls">
                <div class="input-append">
                    @Html.TextBoxFor(m => m.BinaBedeli, new { @class = "m-wrap autoNumeric", style = "width:90px; text-align:right" })<span class="add-on"> TL</span>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row-fluid">
    <div class="span6">
        <div class="control-group">
            <label class="control-label">@babonline.AddDecoration</label>
            <div class="controls">
                <div class="input-append">
                    @Html.TextBoxFor(m => m.DekorasyonBedeli, new { @class = "m-wrap autoNumeric", style = "width:90px; text-align:right" })<span class="add-on"> TL</span>
                </div>
                @Html.ValidationMessageFor(s => s.DekorasyonBedeli)
            </div>
        </div>
    </div>
    <div class="span6">
        <div class="control-group">
            <label class="control-label">@babonline.AddToCommodity</label>
            <div class="controls">
                <div class="input-append">
                    @Html.TextBoxFor(m => m.EmteaBedeli, new { @class = "m-wrap autoNumeric", style = "width:90px; text-align:right" })<span class="add-on"> TL</span>
                </div>
            </div>
        </div>
    </div>
</div>