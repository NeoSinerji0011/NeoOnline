@model Neosinerji.BABOnlineTP.Web.Areas.Teklif.Models.OnerilenUrunModel
@using Neosinerji.BABOnlineTP.Web.Content.Lang


<div id="urun-modal" class="modal hide fade">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    </div>

    <div class="modal-body">
        <table class="table table-bordered table-striped responsive data-table">
            <tr>
                <th>Ürün Kodu</th>
                <th>Ürün Adı</th>
                <th>Sigorta Yaptırma Olasılığı(%) </th>
            </tr>
            @foreach (var item in Model.list)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.policyCode)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.policyName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.probability)
                    </td>
                </tr>
            }
        </table>
    </div>
    <div class="modal-footer">
        <a href="#" class="btn" data-dismiss="modal" aria-hidden="true">Kapat</a>
    </div>
</div>