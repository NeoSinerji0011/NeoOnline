@using Neosinerji.BABOnlineTP.Web.Content.Lang;
@model Neosinerji.BABOnlineTP.Web.Areas.Teklif.Models.DaskRizikoAdresModel



@Html.HiddenFor(s => s.Latitude)
@Html.HiddenFor(s => s.Longitude)
@Html.HiddenFor(s => s.UATVKodu)

@Html.HiddenFor(s => s.Il)
@Html.HiddenFor(s => s.Ilce)
@Html.HiddenFor(s => s.SemtBelde)
@Html.HiddenFor(s => s.Mahalle)
@Html.HiddenFor(s => s.Cadde)
@Html.HiddenFor(s => s.Daire)
@Html.HiddenFor(s => s.Bina)


<div id="paritus-adres-dogrulama">
    <div class="row-fluid">
        <div class="span9">
            <div class="control-group">
                <label class="control-label">@babonline.VerifyAddress</label>
                <div class="controls">
                    @Html.TextBoxFor(s => s.ParitusAdresDogrulama, new {placeholder="UAVT Adres Kodu Giriniz.", style = "width:100%", tabindex = "60" })
                </div>
            </div>
        </div>
        <div>
             <button type="button" style="float: left" class="btn red" id="uavt-sorgula" data-loading-text="@babonline.QueryIndicator" tabindex="61">Adres Detay @babonline.Query</button>
        </div>
        <div class="span3" style="display:none;">
            <button type="button" style="float: left" class="btn red" id="paritus-dogrula" tabindex="61">@babonline.Verify</button>
            <div style="width: 67px; margin-left: 10px; float: left;">
                <span style="color: red; font-size: 11px;">Powered By &nbsp;</span>
                <img width="70px" style="width: 70px;" src="~/Content/img/parituslogo-ori.png" />
            </div>
        </div>
    </div>

    <div class="row-fluid">
        <div class="span9 pull-center">
            <div class="span7">
                <div id="dogrulama-alert-div" class="alert alert-success" style="display: none">
                    <strong>@babonline.VerificationScore </strong><span id="dogrulama-skoru"></span>/1000
                </div>
            </div>
            <div style="text-align: right" class="span5">
                <a href="javascript:" style="color: red" id="ornek">@babonline.Sample</a> <span class="spacer">//</span>
                <a href="javascript:" style="color: red" id="temizle">@babonline.Clear</a>
            </div>
        </div>
    </div>

    <div class="row-fluid" id="paritus-coklu-adres" style="display: none">
        <div class="span9">
            <div style="padding: 3%; text-align: center; display: block;" class="span9 offset1 pull-center caption" id="recommendationdiv">
                <h4 id="infolbl">@babonline.TheAddressYouAreLooking</h4>
                <table style="background-color: #ffffff" class="table table-hover" id="tablerecommendation">
                    <tbody id="tablebodycommendation">
                    </tbody>
                </table>
            </div>
        </div>
    </div>


    <div id="UATV_Adres">
        <div class="row-fluid">
            <div class="span6">
                <div class="control-group">
                    <label class="control-label">@babonline.City<span class="required">*</span></label>
                    <div class="controls">
                        @Html.DropDownListFor(m => m.IlKodu, Model.Iller, new { tabindex = 10, @class = "chosen" })
                        @Html.ValidationMessageFor(m => m.IlKodu)
                    </div>
                </div>
            </div>
         
            <div class="span6">
                <div class="control-group">
                    <label class="control-label">@babonline.AvenueStreetSquare<span class="required">*</span></label>
                    <div class="controls">
                        @Html.DropDownListFor(m => m.CaddeKodu, Model.Caddeler, new { tabindex = 14, @class = "chosen adres-bilgileri" })
                        @Html.ValidationMessageFor(m => m.CaddeKodu)
                    </div>
                </div>
            </div>
        </div>

        <div class="row-fluid">
            <div class="span6">
                <div class="control-group">
                    <label class="control-label">@babonline.District<span class="required">*</span></label>
                    <div class="controls">
                        @Html.DropDownListFor(m => m.IlceKodu, Model.Ilceler, new { tabindex = 11, @class = "chosen adres-bilgileri" })
                        @Html.ValidationMessageFor(m => m.IlceKodu)
                    </div>
                </div>
            </div>
            <div class="span6">
                <div class="control-group">
                    <label class="control-label">@babonline.Building_No<span class="required">*</span></label>
                    <div class="controls">
                        @Html.DropDownListFor(m => m.BinaKodu, Model.Binalar, new { tabindex = 15, @class = "chosen adres-bilgileri" })
                        @Html.ValidationMessageFor(m => m.BinaKodu)
                    </div>
                </div>
            </div>
        </div>

        <div class="row-fluid">
            <div class="span6">
                <div class="control-group">
                    <label class="control-label">@babonline.DistrictTowns<span class="required">*</span></label>
                    <div class="controls">
                        @Html.DropDownListFor(m => m.SemtBeldeKodu, Model.Beldeler, new { tabindex = 12, @class = "chosen adres-bilgileri" })
                        @Html.ValidationMessageFor(m => m.SemtBeldeKodu)
                    </div>
                </div>
            </div>
            <div class="span6">
                <div class="control-group">
                    <label class="control-label">@babonline.Apartment_No<span class="required">*</span></label>
                    <div class="controls">
                        @Html.DropDownListFor(m => m.DaireKodu, Model.Daireler, new { tabindex = 16, @class = "chosen adres-bilgileri" })
                        @Html.ValidationMessageFor(m => m.DaireKodu)
                    </div>
                </div>
            </div>
        </div>

        <div class="row-fluid">
            <div class="span6">
                <div class="control-group">
                    <label class="control-label">@babonline.Parish<span class="required">*</span></label>
                    <div class="controls">
                        @Html.DropDownListFor(m => m.MahalleKodu, Model.Mahalleler, new { tabindex = 13, @class = "chosen adres-bilgileri" })
                        @Html.ValidationMessageFor(m => m.MahalleKodu)
                    </div>
                </div>
            </div>
            <div class="span6">
                <div class="control-group">
                    <label class="control-label">@babonline.Postal_Code <span class="required">*</span></label>
                    <div class="controls">
                        @Html.TextBoxFor(m => m.PostaKodu, new { tabindex = 17, maxlength = 5, minlength = 5, pattern = ".{5,5}", title = babonline.Message_PostCode, autocompletetype = "disabled", style = "width:70px" })
                        @Html.ValidationMessageFor(m => m.PostaKodu)
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="adres-info" class="alert alert-info" style="display: none">
        <button class="close" data-dismiss="alert"></button>
        <strong>@babonline.Address_Info2</strong> <span id="adres-text"></span>
    </div>

    <div class="row-fluid">
        <div class="span6"></div>
        <div class="span6">
            <div class="accordion-group">
                <div class="accordion-heading">
                    <a class="accordion-toggle collapsed" data-toggle="collapse" data-parent="#accordion1" href="#collapse_2">
                        <i class="icon-angle-left"></i>
                        @babonline.Map
                    </a>
                </div>
                <div id="collapse_2" class="accordion-body collapse">
                    <div class="accordion-inner">
                        <div id="gmap_marker" class="gmaps"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>





