@using Neosinerji.BABOnlineTP.Web.Content.Lang
@model Neosinerji.BABOnlineTP.Web.Areas.Manage.Models.KullaniciModel

<div id="kilitle-modal" class="modal hide fade">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
        @if (Model.Durum == 2)
        {<h3>@babonline.UnBlock</h3>}
        else
        {<h3>@babonline.Block</h3>}
    </div>
    <div class="modal-body">
        @if (Model.Durum == 2)
        {<p>@babonline.UnBlock_Info</p> }
        else
        {<p>@babonline.Block_Info</p> }
    </div>
    <div class="modal-footer">
        <a href="#" class="btn btn-info" id="btn-onay" data-dismiss="modal" aria-hidden="true">@babonline.OK</a>
        <a href="#" class="btn btn-info" data-dismiss="modal" aria-hidden="true">@babonline.Cancel</a>
    </div>
</div>
