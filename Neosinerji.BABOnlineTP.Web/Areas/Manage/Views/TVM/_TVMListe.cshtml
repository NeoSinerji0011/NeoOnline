@using Neosinerji.BABOnlineTP.Web.Content.Lang
@using Neosinerji.BABOnlineTP.Web.Areas.Teklif.Models
@using Neosinerji.BABOnlineTP.Business
@model List<TVMOzetModel>

<table id="tvm-table" class="table table-bordered table-striped responsive data-table">
    <thead>
        <tr>
            <th style="width: 100px;"></th>
            <th style="width: 200px;">@babonline.TVM_Code</th>
            <th style="width: 300px;">@babonline.TVM_Title</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="text-align: center;"><a class="btn btn-mini btn-success tvm-sec-btn" tvm-kodu="@item.Kodu" tvm-unvani="@item.Unvani">@babonline.Choose</a></td>
                <td>@item.Kodu</td>
                <td>@item.Unvani</td>
            </tr>
        }
    </tbody>
</table>
