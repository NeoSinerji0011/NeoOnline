@using Neosinerji.BABOnlineTP.Web.Content.Lang
@using Neosinerji.BABOnlineTP.Web.Tools.Helpers
@using Neosinerji.BABOnlineTP.Business.Common
@using Neosinerji.BABOnlineTP.Web.Areas.Rapor.Models
@model Neosinerji.BABOnlineTP.Web.Areas.Rapor.Models.TVMProjeKoduGenelModel
@using Neosinerji.BABOnlineTP.Web


@{
    var lang = System.Threading.Thread.CurrentThread.CurrentUICulture.TwoLetterISOLanguageName;
}


@section PageStyles
{
    <link rel="stylesheet" href="~/Content/css/daterangepicker.css" />
    <link href="~/content/css/jquery.gritter.css" rel="stylesheet" type="text/css" />
    @*     <link href="~/Content/css/style.css" rel="stylesheet" />*@
    <style type="text/css">
        .aktif-pasif {
            color: blue;
            position: relative;
            font-size: medium;
            margin-left: 25px;
            height: 25px;
        }
        /*hr {
          width: 540px;
        }*/
    </style>
}

@section HorizontalMenu
{
    <ul class="nav">

        <li class="active">
            <a href="#">Satış Kanalı ve Kullancı Adetleri<span class="selected"></span></a>
        </li>
        <li>
            <span class="hor-menu-search-form-toggler">&nbsp;</span>
            <div class="search-form hidden-phone hidden-tablet">
                <form class="form-search">
                    <div class="input-append">
                        <input type="text" placeholder="@babonline.Search" class="m-wrap">
                        <button type="button" class="btn"></button>
                    </div>
                </form>
            </div>
        </li>
    </ul>
}
@section PageHeader
{

}
@section PageScripts
{
    <script>
        $(document).ready(function () {

        });

    </script>

}
<div class="portlet box light-grey" style="margin-top: 24px;">
    <div class="portlet-title">
        <div class="caption"><i class="icon-reorder"></i>Satış Kanalı ve Kullancı Adetleri</div>
        <div class="tools">
            <a href="javascript:;" class="collapse"></a>
        </div>
    </div>
    <div class="portlet-body">
        <div class="row-fluid">
            <div class="span4">
                <div class="portlet box green" style="height: auto;">
                    <div class="portlet-title">
                        <div class="caption"><i class="icon-reorder"></i>Acente Sayısı</div>
                    </div>
                    <div class="portlet-body">
                        <div class="row-fluid">
                            <div class="span6">
                                <label class="control-label" style="color: red; position: relative; margin-left: 60px; font-size: medium">Acente Sayısı</label>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <div class="span6">
                                <div class="control-group">
                                    <label class="control-label aktif-pasif">@babonline.Active <span style="color: red; position: relative; margin-left: 50px;">@Model.RaporSonuc.AktifAcenteSayisi</span></label>
                                    <label class="control-label aktif-pasif">Pasif <span style="color: red; position: relative; margin-left: 50px;">@Model.RaporSonuc.PasifAcenteSayisi</span></label>
                                    <label class="control-label aktif-pasif">Toplam <span style="color: red; position: relative; margin-left: 35px;">@Model.ToplamAcenteSayisi</span></label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="span4">
                <div class="portlet box purple">
                    <div class="portlet-title">
                        <div class="caption"><i class="icon-reorder"></i>Kullanıcı Sayısı</div>

                    </div>
                    <div class="portlet-body">
                        <div class="row-fluid">
                            <div class="span6">
                                <label class="control-label" style="color: red; position: relative; margin-left: 60px; font-size: medium">Kullanıcı Sayısı</label>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <div class="span6">
                                <div class="control-group">
                                    <label class="control-label aktif-pasif">Aktif <span style="color: red; position: relative; margin-left: 50px;">@Model.RaporSonuc.AktifKullaniciSayisi</span></label>
                                    <label class="control-label aktif-pasif">Pasif <span style="color: red; position: relative; margin-left: 50px;">@Model.RaporSonuc.PasifKullaniciSayisi</span></label>
                                    <label class="control-label aktif-pasif">Toplam <span style="color: red; position: relative; margin-left: 35px;">@Model.ToplamKullaniciSayisi</span></label>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="span4">
                <div class="portlet box blue">
                    <div class="portlet-title">
                        <div class="caption"><i class="icon-reorder"></i>Login Olan Kullanıcı Sayısı</div>

                    </div>
                    <div class="portlet-body">
                        <div class="row-fluid">
                            <div class="span8">
                                <label class="control-label" style="color: red; position: relative; margin-left: 60px; font-size: medium">Login Olan Kullanıcı Sayısı</label>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <div class="span6">
                                <div class="control-group">
                                    <label class="control-label aktif-pasif">Bugün <span style="color: red; position: relative; margin-left: 45px;">@Model.RaporSonuc.LoginKullaniciBugun</span></label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
