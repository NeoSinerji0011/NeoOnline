                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row-fluid">
                        <div class="span6">
                            <div class="control-group">
                                <label class="control-label">@babonline.Branch</label>
                                <div class="controls">
                                    @Html.DropDownListFor(s => s.BransSelectList, Model.BranslarItems, new { @class = "multiselect", multiple = "multiple" })

                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label class="control-label">@babonline.ReportStartDate</label>
                                <div class="controls">
                                    @Html.DateTextBoxFor(s => s.BaslangicTarihi, new { style = "width:100px", Id = "BaslangisTarihi" })

                                    @Html.ValidationMessageFor(s => s.BaslangicTarihi)
                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label class="control-label">@babonline.ReportEndDate</label>
                                <div class="controls">
                                    @Html.DateTextBoxFor(s => s.BitisTarihi, new { style = "width:100px", Id = "BitisTarihi" })
                                    @Html.ValidationMessageFor(s => s.BitisTarihi)
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row-fluid">
                        <div class="span6">
                            <div class="control-group">
                                <label class="control-label">@babonline.InsuranceCompany</label>
                                <div class="controls">
                                    @Html.DropDownListFor(s => s.SigortaSirketleriSelectList, Model.SigortaSirketleri, new { @class = "multiselect", multiple = "multiple" })

                                </div>
                            </div>
                        </div>
                        <div class="span6">
                            <div class="control-group">
                                <label class="control-label">@babonline.PolicyNo</label>
                                <div class="controls">
                                    @Html.TextBoxFor(s => s.PoliceNo, new { style = "width:90%" })
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row-fluid">
                        <div class="span6">
                            <div class="control-group">
                                <label class="control-label">@babonline.PolicyDateType</label>
                                <div class="controls">
                                    @Html.DropDownListFor(s => s.PoliceTarihTipi, Model.PoliceTarihiTipleri.Take(2).Reverse(), new { style = "width:81%" })
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row-fluid">
                        <div class="span6">
                            <div class="control-group">
                                <label class="control-label">@babonline.PaymentStyle</label>
                                <div class="controls">
                                    @Html.DropDownListFor(s => s.OdemeSekli, Model.OdemeSekilleri, new { style = "width:81%" })
                                </div>
                            </div>
                        </div>

                    </div>

                    <div class="row-fluid">
                        <div class="span6">
                            <div class="control-group">
                                <label class="control-label">@babonline.PaymentType</label>
                                <div class="controls">
                                    @Html.DropDownListFor(s => s.OdemeTipi, Model.OdemeTipleri, new { style = "width:81%" })
                                </div>
                            </div>
                        </div>


                    </div>

                    <div id="alert" class="alert fade" style="display: none">
                        <button type="button" class="close" id="close-alert">×</button>
                        <strong>@babonline.Warning</strong>  @babonline.Message_Customer_SearchError
                    </div>
                    <div class="form-actions">
                        <a href="~/Rapor/Rapor/PoliceListesi" class="btn ligth-grey">@babonline.Clear</a>
                        <input type="submit" id="PoliceListesiOfflineSearch" class="btn btn-success" data-loading-text="@babonline.QueryIndicator" value="@babonline.Search" />

                    </div>

                }
                <script src="~/Scripts/jquery-1.8.2.min.js"></script>


                <div class="row-fluid" style="margin-top: 30px;">

                    <div class="span12">
                        <div id="table-div" style="overflow-x: auto; margin-top: 15px;">
                            @if (ViewBag.AnaTVM)
                            {
                                <table class="table table-striped table-bordered table-hover dataTable data-table" style="table-layout: auto; width: 100%;">
                                    <thead>
                                        <tr>
                                            <th style="width: 15%;">@babonline.PolicyNo / @babonline.RenewalNo / @babonline.Endorsement </th>
                                            <th style="width: 5%;">@babonline.Insured</th>
                                            <th style="width: 5%;">@babonline.Insured <br />TCKN/VKN</th>
                                            <th style="width: 5%;">@babonline.InsuredBy</th>
                                            <th style="width: 15%;">@babonline.InsuranceCompany</th>
                                            <th style="width: 4%;">@babonline.Branch</th>
                                            <th style="width: 8%;">Plaka / İl-İlçe Adı</th>
                                            <th style="width: 4%;">@babonline.Date_Of_the_Establishment</th>
                                            <th style="width: 4%;">@babonline.PolicyStartDate</th>
                                            <th style="width: 4%;">@babonline.PolicyFinishDate</th>
                                            <th style="width: 3%;">@babonline.Gross_Premium</th>
                                            <th style="width: 3%;">@babonline.Net_Premium</th>
                                            <th style="width: 3%;">Alınan @babonline.Commission</th>
                                            <th style="width: 3%;">KMS % </th>
                                            <th style="width: 3%;">@babonline.SubAgencyComission</th>
                                            <th style="width: 3%;">Dövizli @babonline.Gross_Premium</th>
                                            <th style="width: 3%;">Dövizli @babonline.Net_Premium</th>
                                            <th style="width: 3%;">Dövizli @babonline.Commission</th>
                                            <th style="width: 3%;">@babonline.PaymentType</th>
                                            <th style="width: 3%;">@babonline.PaymentStyle</th>
                                            <th style="width: 3%;">Döviz Kur</th>
                                            <th style="width: 3%;">Para Birimi</th>
                                            <th style="width: 5%;">Satış Kanalı</th>
                                            <th style="width: 5%;">Dış Kaynak Unvanı</th>
                                            <th style="width: 5%;">Onaylayan Personel Adı Soyadı</th>
                                            <th style="width: 5%;">Sigortalı<br />E-Mail</th>
                                            <th style="width: 5%;">Muhasebeye işlendi mi</th>
                                            <th style="width: 5%;">Manuel Poliçe mi?</th>
                                            <th style="width: 5%;">Yeni İş</th>

                                        </tr>
                                    </thead>
                                    @{
                                        decimal? toplamBrut = 0;
                                        decimal? toplamBrutTahakkuk = 0;
                                        decimal? toplamBrutIptal = 0;
                                        decimal? toplamNet = 0;
                                        decimal? toplamNetTahakkuk = 0;
                                        decimal? toplamNetIptal = 0;
                                        decimal? toplamKomisyon = 0;
                                        decimal? toplamKomisyonTahakkuk = 0;
                                        decimal? toplamKomisyonIptal = 0;
                                        decimal? toplamTaliKomisyon = 0;
                                        decimal? toplamTaliKomisyonTahakkuk = 0;
                                        decimal? toplamTaliKomisyonIptal = 0;
                                        decimal? toplamDolarBrut = 0;
                                        decimal? toplamDolarNet = 0;
                                        decimal? toplamDolarKomisyon = 0;
                                        decimal? toplamDolarTaliKomisyon = 0;
                                        decimal? toplamEuroBrut = 0;
                                        decimal? toplamEuroNet = 0;
                                        decimal? toplamEuroKomisyon = 0;
                                        decimal? toplamEuroTaliKomisyon = 0;
                                        decimal? toplamGBPBrut = 0;
                                        decimal? toplamGBPNet = 0;
                                        decimal? toplamGBPKomisyon = 0;
                                        decimal? toplamGBPTaliKomisyon = 0;
                                        decimal? GenelPolToplamSayac = 0;
                                        decimal? GenelZeylToplamSayac = 0;

                                    }
                                    <tbody>
                                        @if (Model.procedurePoliceOfflineList != null && Model.procedurePoliceOfflineList.Count > 0)
                                        {

                                            var kontrol = 0;
                                            decimal? dovizliBrut = 0;
                                            foreach (var item in Model.procedurePoliceOfflineList)
                                            {
                                                kontrol = 0;
                                                <tr>
                                                    <td style="width: 10%;">@item.PoliceNumarasi / @item.YenilemeNo / @item.EkNo</td>

                                                    <td style="width: 10%;">@item.SliUnvan</td>
                                                    <td style="width: 10%;">@item.TcknVkn</td>
                                                    <td style="width: 10%;">@item.SEUnvan</td>
                                                    <td style="width: 15%;">@item.SirketAdi</td>
                                                    <td style="width: 10%;">@item.BransAdi</td>
                                                    @if (item.BransKodu == 1 || item.BransKodu == 2)
                                                    {
                                                        <td style="width: 10%;">@item.PlakaKodu  @item.PlakaNo </td>
                                                    }
                                                    else if (item.BransKodu > 0)
                                                    {
                                                        <td style="width: 10%;">@item.PsliIlAdi  @item.PsliIlceAdi </td>

                                                    }
                                                    else
                                                    {
                                                        <td style="width: 3%;"></td>
                                                    }
                                                    @if (item.TanzimTarihi.HasValue)
                                                    {
                                                        <td style="width: 10%;">
                                                            @item.TanzimTarihi.Value.ToShortDateString()
                                                        </td>
                                                    }
                                                    else
                                                    {
                                                        <td style="width: 10%;"> </td>
                                                    }
                                                    @if (item.BaslangicTarihi.HasValue)
                                                    {
                                                        <td style="width: 10%;">
                                                            @item.BaslangicTarihi.Value.ToShortDateString()
                                                        </td>
                                                    }
                                                    else
                                                    {
                                                        <td style="width: 10%;"> </td>
                                                    }
                                                    @if (item.BitisTarihi.HasValue)
                                                    {
                                                        <td style="width: 10%;">
                                                            @item.BitisTarihi.Value.ToShortDateString()
                                                        </td>
                                                    }
                                                    else
                                                    {
                                                        <td style="width: 10%;"> </td>
                                                    }

                                                    <td style="width: 5%;">@item.BrutPrim.Value.ToString("N")</td>
                                                    @{
                                                        for (int i = 0; i < Model.procedurePoliceOfflineList.Count(); i++)
                                                        {
                                                            kontrol++;
                                                            if (kontrol == 1)
                                                            {
                                                                if (@item.BrutPrim.Value > 0)
                                                                {
                                                                    toplamBrutTahakkuk += @item.BrutPrim.Value;
                                                                    toplamNetTahakkuk += @item.NetPrim.Value;
                                                                    toplamKomisyonTahakkuk += @item.Komisyon.Value;
                                                                    toplamTaliKomisyonTahakkuk += @item.TaliKomisyon.Value;


                                                                }
                                                                else
                                                                {
                                                                    toplamBrutIptal += @item.BrutPrim.Value;
                                                                    toplamNetIptal += @item.NetPrim.Value;
                                                                    toplamKomisyonIptal += @item.Komisyon.Value;
                                                                    toplamTaliKomisyonIptal += @item.TaliKomisyon.Value;
                                                                }
                                                                toplamBrut += @item.BrutPrim.Value;
                                                                toplamNet += @item.NetPrim.Value;
                                                                toplamTaliKomisyon += @item.TaliKomisyon.Value;
                                                                toplamKomisyon += @item.Komisyon.Value;
                                                            }
                                                            if (kontrol == 1)
                                                            {
                                                                if (item.ParaBirimi == "USD")
                                                                {
                                                                    if (@item.BrutPrim != 0 && @item.DovizKur != 0)
                                                                    {
                                                                        decimal? bru = @item.BrutPrim;
                                                                        decimal? kurr = @item.DovizKur;

                                                                        toplamDolarBrut += kurr != null ? bru / kurr : 0;
                                                                    }
                                                                    if (@item.NetPrim != 0 && @item.DovizKur != 0)
                                                                    {
                                                                        decimal? net = @item.NetPrim;
                                                                        decimal? kurr = @item.DovizKur;

                                                                        toplamDolarNet += kurr != null ? net / kurr : 0;
                                                                    }
                                                                    if (@item.TaliKomisyon != 0 && @item.DovizKur != 0)
                                                                    {
                                                                        decimal? taliKomisyon = @item.TaliKomisyon;
                                                                        decimal? kurr = @item.DovizKur;

                                                                        toplamDolarTaliKomisyon += kurr != null ? taliKomisyon / kurr : 0;
                                                                    }
                                                                    if (@item.Komisyon != 0 && @item.DovizKur != 0)
                                                                    {
                                                                        decimal? komisyon = @item.Komisyon;
                                                                        decimal? kurr = @item.DovizKur;

                                                                        toplamDolarKomisyon += kurr != null ? komisyon / kurr : 0;
                                                                    }

                                                                }
                                                                if (item.ParaBirimi == "EUR")
                                                                {
                                                                    if (@item.BrutPrim != 0 && @item.DovizKur != 0)
                                                                    {
                                                                        decimal? bru = @item.BrutPrim;
                                                                        decimal? kurr = @item.DovizKur;

                                                                        // toplamEuroBrut += kurr != null ? bru / kurr : 0;
                                                                        toplamEuroBrut += kurr != null ? item.DovizliBrutPrim : 0;
                                                                    }
                                                                    if (@item.NetPrim != 0 && @item.DovizKur != 0)
                                                                    {
                                                                        decimal? net = @item.NetPrim;
                                                                        decimal? kurr = @item.DovizKur;

                                                                        //toplamEuroNet += kurr != null ? net / kurr : 0;
                                                                        toplamEuroNet += kurr != null ? item.DovizliNetPrim : 0;
                                                                    }
                                                                    if (@item.TaliKomisyon != 0 && @item.DovizKur != 0)
